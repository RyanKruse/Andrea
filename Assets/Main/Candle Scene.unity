    public TextMeshProUGUI HELVETICA_TMP;\r\n    public TextMeshProUGUI HELVETICA_TMP_DOUBLE;\r\n   
    public TextMeshProUGUI TAHOMA_TMP;\r\n    public TextMeshProUGUI TAHOMA_TMP_DOUBLE;\r\n  
    public TextMeshProUGUI BASKERVILLE_TMP;\r\n    public TextMeshProUGUI BASKERVILLE_TMP_DOUBLE;\r\n  
    public TextMeshProUGUI PAGE_V_TMP;\r\n    public TextMeshProUGUI PERCENTAGE_V_TMP;\r\n  
    public TextMeshProUGUI PAGE_H_TMP;\r\n    public TextMeshProUGUI PERCENTAGE_H_TMP;\r\n  
    public TextMeshProUGUI PAGE_TMP;\r\n    public TextMeshProUGUI PERCENTAGE_TMP;\r\n  
    public int[] SLICE_LIST;\r\n    public int[] HEAD_SIZE_LIST;\r\n    public int[]
    CORE_SIZE_LIST;\r\n    public int[] LEAD_SIZE_LIST;\r\n    public int[] BODY_SIZE_LIST;\r\n   
    public string[] FONT_TYPE_LIST;\r\n    public float[] BULLET_SIZE_LIST;\r\n  
    public bool[] RICH_TEXT_BOOL_OVERFLOW = new bool[10];\r\n    public string[]
    RICH_TEXT_STRING_OVERFLOW = new string[10];\r\n    public string[] RICH_TEXT_OVERFLOW;\r\n  
    public float[] CANVAS_SIZES;\r\n    public int blockIndex;\r\n    public int
    fontSize;\r\n    public string fontType;\r\n    public int pageLength;\r\n   
