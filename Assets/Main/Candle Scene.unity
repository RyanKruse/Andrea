  m_text: "using UdonSharp;\r\nusing UnityEngine;\r\nusing VRC.SDKBase;\r\nusing
    VRC.Udon;\r\nusing System.IO;\r\nusing UnityEditor;\r\nusing TMPro;\r\nusing
    System.Text.RegularExpressions;\r\nusing UnityEngine.UI;\r\nusing System.Reflection;\r\nusing
    System;\r\nusing System.Linq;\r\n\r\npublic class Kruse_ : UdonSharpBehaviour\r\n{\n   
    public bool IS_PRECOMPILED_TEXT;\r\n   public bool IS_APPEND_FLUSH;\r\n    public
    int DEFAULT_BLOCK_INDEX;\r\n    public int DEFAULT_FONT_SIZE;\r\n    public int
    DEFAULT_FONT_TYPE;\r\n    public string DEFAULT_TABLET_ORIENTATION;\r\n    public
    TextAsset RICH_TEXT_FORMAT;\r\n    public TextAsset TEXT;\r\n    public TextAsset
    BLOCK;\r\n    public GameObject CANDLE;\r\n   public VRC_Pickup CANDLE_PICKUP;\r\n   
    public GameObject CANDLE_GAMEOBJECT;\r\n   public GameObject DOUBLE_GAMEOBJECT;\r\n   
    public GameObject HELVETICA_GAMEOBJECT;\r\n   public GameObject HELVETICA_DOUBLE_GAMEOBJECT;\r\n   
    public GameObject TAHOMA_GAMEOBJECT;\r\n   public GameObject TAHOMA_DOUBLE_GAMEOBJECT;\r\n   
    public GameObject BASKERVILLE_GAMEOBJECT;\r\n   public GameObject BASKERVILLE_DOUBLE_GAMEOBJECT;\r\n   
    public TextMeshProUGUI KRUSE_TMP;\r\n    public TextMeshProUGUI DOUBLE_TMP;\r\n   
    public TextMeshProUGUI HELVETICA_TMP;\r\n    public TextMeshProUGUI HELVETICA_TMP_DOUBLE;\r\n   
    public TextMeshProUGUI TAHOMA_TMP;\r\n    public TextMeshProUGUI TAHOMA_TMP_DOUBLE;\r\n  
    public TextMeshProUGUI BASKERVILLE_TMP;\r\n    public TextMeshProUGUI BASKERVILLE_TMP_DOUBLE;\r\n  
    public TextMeshProUGUI PAGE_V_TMP;\r\n    public TextMeshProUGUI PERCENTAGE_V_TMP;\r\n  
    public TextMeshProUGUI PAGE_H_TMP;\r\n    public TextMeshProUGUI PERCENTAGE_H_TMP;\r\n  
    public TextMeshProUGUI PAGE_TMP;\r\n    public TextMeshProUGUI PERCENTAGE_TMP;\r\n  
    public int[] SLICE_LIST;\r\n    public int[] HEAD_SIZE_LIST;\r\n    public int[]
    CORE_SIZE_LIST;\r\n    public int[] LEAD_SIZE_LIST;\r\n    public int[] BODY_SIZE_LIST;\r\n   
    public string[] FONT_TYPE_LIST;\r\n    public float[] BULLET_SIZE_LIST;\r\n  
    public bool[] RICH_TEXT_BOOL_OVERFLOW = new bool[10];\r\n    public string[]
    RICH_TEXT_STRING_OVERFLOW = new string[10];\r\n    public string[] RICH_TEXT_OVERFLOW;\r\n  
    public float[] CANVAS_SIZES;\r\n    public int blockIndex;\r\n    public int
    fontSize;\r\n    public string fontType;\r\n    public int pageLength;\r\n   
    public int pageIndex;\r\n    public int charZeroIndex;\r\n    public string tabletOrientation;\r\n  
    public string statusCode;\r\n    public bool isRich = false;\r\n    public string
    richCommand = \"\";\r\n    public bool emergencyUpdate = false;\r\n    private
    int startIndex = 0;\r\n    public Memory_ books_;\r\n    public char[] badChars
    = { '\\n', ' ', '\\t' };\r\n    public char[] bracketCharacters = { ')', '.',
    '-', ':', '|', '\u2022' };\r\n    public string[] FORMAT_FLAGS;\r\n    public
    string[] FORMAT_BRACKETS;\r\n    public int overflowAudit = -1;\r\n    public
    int endCalibrationMemory = 0;\r\n    public bool isRightHandActive = false;\r\n  
    public bool isLeftHandActive = false;\r\n    public bool isScrollWheelEnabled
    = false;\r\n\r\n    void Start()\r\n    {\r\n        if (IS_PRECOMPILED_TEXT)
    return;\r\n        PAGE_H_TMP.text = \"\";\r\n        PERCENTAGE_H_TMP.text =
    \"\";\r\n        PAGE_TMP = PAGE_V_TMP;\r\n        PERCENTAGE_TMP = PERCENTAGE_V_TMP;\r\n      
    PopulateRichTextFormat();\r\n        DefineDefaultState();\r\n        Calibrate(0);\r\n      
    emergencyUpdate = true;\r\n    }\r\n\r\n    public void PopulateRichTextFormat()\r\n  
    {\r\n        string fileContexts = RICH_TEXT_FORMAT.text;\r\n\r\n        int
    count = 0;\r\n\r\n        // Count how many command rows we have in RICH_TEXT_FORMAT.
    //\r\n        // Don't add any more new lines to Rich Text Format.\r\n       
    for (int i = 0; i < fileContexts.Length - 1; i++)\r\n        {\r\n           
    char c = fileContexts[i];\r\n            if (c == '\\n' && fileContexts[i + 1]
    != '\\n')\r\n           {\r\n                count++;\r\n            }\r\n       
    }\r\n\r\n       FORMAT_FLAGS = new string[count];\r\n        FORMAT_BRACKETS
    = new string[count];\r\n\r\n       int state = 0;\r\n        int index = 0;\r\n       
    string text = \"\";\r\n\r\n       // Populate arrays with commands.  // \r\n       
    for (int i = 0; i < fileContexts.Length; i++)\r\n        {\r\n            char
    character = fileContexts[i];\r\n           if (character == '\\n')\r\n           
    {\r\n                Debug.Log($\"State ({state}): {text}\");\r\n               
    if (state == 3)\r\n                {\r\n                   FORMAT_BRACKETS[index]
    = text.Substring(0, text.Length - 1);  // Warning invisible \\n character.\r\n                   
    index++;\r\n                    text = \"\";\r\n                }\r\n               
    state = 0;\r\n            }\r\n           else if (state == 0 && (character ==
    '<' || char.IsLetterOrDigit(character) || character == '\\\\'))\r\n           
    {\r\n                state = 1;\r\n            }\r\n           else if (character
    == '\\t')\r\n            {\r\n                if (state == 1)\r\n               
    {\r\n                    FORMAT_FLAGS[index] = text;\r\n                    text
    = \"\";\r\n                }\r\n                state = 2;\r\n            }\r\n           
    else if (state == 2 && (character == '<' || char.IsLetterOrDigit(character) ||
    character == '\\\\' || character == '\u200B'))\r\n           {\r\n               
    state = 3;\r\n            }\r\n\r\n            if (state == 1 || state == 3)\r\n           
    {\r\n                text += character;\r\n           }\r\n        }\r\n    }\r\n\r\n   
    public void DefineDefaultState()\r\n   {\r\n        blockIndex = DEFAULT_BLOCK_INDEX;\r\n       
    fontSize = BODY_SIZE_LIST[DEFAULT_FONT_SIZE];\r\n       fontType = FONT_TYPE_LIST[DEFAULT_FONT_TYPE].Trim(badChars);\r\n       
    tabletOrientation = DEFAULT_TABLET_ORIENTATION;\r\n    }\r\n\r\n    public void
    Calibrate(int increment)\r\n   {\r\n        endCalibrationMemory = pageIndex;\r\n       
    pageIndex = 0;\r\n       DefineActiveTMP();\r\n        DefineStatusCode();\r\n       
    DefinePaths(increment);\r\n       DefineBlock();\r\n        DefineSlice();\r\n       
    DefineOverflow();\r\n   }\r\n\r\n    public void DefineActiveTMP() // \r\n   
    {\r\n        if (KRUSE_TMP != null)\r\n        {\r\n            KRUSE_TMP.text
    = \"\";\r\n            DOUBLE_TMP.text = \"\";\r\n        }\r\n        if (DEFAULT_FONT_TYPE
    == 0)\r\n        {\r\n            RectTransformAudit(HELVETICA_TMP, HELVETICA_TMP_DOUBLE);\r\n           
    KRUSE_TMP = HELVETICA_TMP;\r\n            DOUBLE_TMP = HELVETICA_TMP_DOUBLE;\r\n           
    CANDLE_GAMEOBJECT = HELVETICA_GAMEOBJECT;\r\n           DOUBLE_GAMEOBJECT = HELVETICA_DOUBLE_GAMEOBJECT;\r\n       
    }   \r\n       else if (DEFAULT_FONT_TYPE == 1)\r\n        {\r\n            RectTransformAudit(TAHOMA_TMP,
    TAHOMA_TMP_DOUBLE);\r\n            KRUSE_TMP = TAHOMA_TMP;\r\n            DOUBLE_TMP
    = TAHOMA_TMP_DOUBLE;\r\n            CANDLE_GAMEOBJECT = TAHOMA_GAMEOBJECT;\r\n          
    DOUBLE_GAMEOBJECT = TAHOMA_DOUBLE_GAMEOBJECT;\r\n        }\r\n        else if
    (DEFAULT_FONT_TYPE == 2)\r\n        {\r\n            RectTransformAudit(BASKERVILLE_TMP,
    BASKERVILLE_TMP_DOUBLE);\r\n            KRUSE_TMP = BASKERVILLE_TMP;\r\n           
    DOUBLE_TMP = BASKERVILLE_TMP_DOUBLE;\r\n            CANDLE_GAMEOBJECT = BASKERVILLE_GAMEOBJECT;\r\n          
    DOUBLE_GAMEOBJECT = BASKERVILLE_DOUBLE_GAMEOBJECT;\r\n        }\r\n   }\r\n\r\n   
    public void RectTransformAudit(TextMeshProUGUI mainText, TextMeshProUGUI doubleText)\r\n   
    {\r\n        if (KRUSE_TMP == null) return;\r\n        RectTransform rectTransform
    = KRUSE_TMP.GetComponent<RectTransform>();\r\n        RectTransform rectTransform2
    = mainText.GetComponent<RectTransform>();\r\n        rectTransform2.sizeDelta
    = rectTransform.sizeDelta;\r\n        rectTransform2.rotation = rectTransform.rotation;\r\n      
    rectTransform2.localPosition = rectTransform.localPosition;\r\n        rectTransform2
    = doubleText.GetComponent<RectTransform>();\r\n        rectTransform2.sizeDelta
    = rectTransform.sizeDelta;\r\n        rectTransform2.rotation = rectTransform.rotation;\r\n      
    rectTransform2.localPosition = rectTransform.localPosition;\r\n    }\r\n\r\n  
    public void DefineStatusCode()\r\n    {\r\n        statusCode = $\"TO:{tabletOrientation},FS:{DEFAULT_FONT_SIZE},FT:{DEFAULT_FONT_TYPE},PC:\";\r\n  
    }\r\n\r\n    public void DefinePaths(int increment)\r\n    {\r\n        blockIndex
    += increment;\r\n        TEXT = books_.THE_SOCIAL_SKILLS_GUIDEBOOK_TEXTS[blockIndex];\r\n      
    BLOCK = books_.THE_SOCIAL_SKILLS_GUIDEBOOK_BLOCKS[blockIndex];\r\n    }\r\n\r\n  
    public void DefineBlock()\r\n    {\r\n        string blockString = BLOCK.text;\r\n      
    int index = blockString.IndexOf(statusCode) + statusCode.Length;\r\n        string
    builder = \"\";\r\n\r\n        // Get page length.\r\n        for (int i = index;
    i < int.MaxValue; i++)\r\n        {\r\n            char temp = BLOCK.text[i];\r\n          
    if (char.IsNumber(temp))\r\n            {\r\n                builder += temp;\r\n           
    }\r\n            else\r\n            {\r\n                pageLength = System.Convert.ToInt32(builder);\r\n               
    SLICE_LIST = new int[pageLength + 2];  // Account for two 0's.\r\n               
    charZeroIndex = i + 2;  // Account for /n and last number of pageLength.\r\n               
    // Debug.Log($\"Page Count: {pageCount} | CharZero: {blockString[charZeroIndex]}\");\r\n               
    return;\r\n           }\r\n        }\r\n    }\r\n\r\n    public void DefineSlice()\r\n   
    {\r\n       int index = 0;\r\n        string builder = \"\";\r\n        for (int
    i = charZeroIndex; i < BLOCK.text.Length; i++)\r\n        {\r\n            char
    temp = BLOCK.text[i];\r\n            if (temp != '\\n')\r\n            {\r\n               
    builder += temp;\r\n            }\r\n            else\r\n            {\r\n               
    // Debug.Log($\"Index {index}: [{builder}]\");\r\n                int k;\r\n               
    if (int.TryParse(builder, out k))\r\n                {\r\n                   
    SLICE_LIST[index] = System.Convert.ToInt32(builder);\r\n                    index++;\r\n                   
    builder = \"\";\r\n                }\r\n                else return;\r\n           
    }\r\n       }\r\n    }\r\n\r\n    public void DefineOverflow()\r\n    {\r\n       
    RICH_TEXT_OVERFLOW = new string[pageLength];\r\n        pageIndex = 0;\r\n       
    Debug.Log($\"DefineOverflow() | Text: {TEXT.name} | Length: {pageLength} | CalibrationMemory:
    {endCalibrationMemory}\");\r\n       overflowAudit = 0;\r\n        //CANDLE_TMP.text
    = \"\";\r\n        //DOUBLE_TMP.text = \"\";\r\n        CANDLE_GAMEOBJECT.SetActive(false);\r\n       
    DOUBLE_GAMEOBJECT.gameObject.SetActive(false);\r\n   }\r\n\r\n    public void
    LoopOverflowAudit()\r\n    {\r\n        if (overflowAudit < pageLength)\r\n       
    {\r\n            pageIndex = overflowAudit;\r\n\r\n\r\n           KRUSE_TMP.text
    = \"\";\r\n\r\n            // Debug.Log($\"pageIndex: {pageIndex} | SLICE_LIST[pageIndex]:
    {SLICE_LIST[pageIndex]} | SLICE_LIST[pageIndex + 1]: {SLICE_LIST[pageIndex +
    1]} | Slice Length: {SLICE_LIST[pageIndex + 1] - SLICE_LIST[pageIndex]}\");\r\n           
    \r\n            // Overflow cutoffs okay, need </indent> at end of bullet sentence.
    Hard, because doesn't know if bullet or body paragraph with <align=\"left\"><indent=2.3em>...
    and those commands can change from richtextformatting.txt\r\n            KRUSE_TMP.text
    += TEXT.text.Substring(pageIndex == 0 ? 0 : SLICE_LIST[pageIndex] + 1, SLICE_LIST[pageIndex
    + 1] - SLICE_LIST[pageIndex]);\r\n\r\n           FormatText(true);\r\n\r\n           
    // Insert index 1 rich text after SuperFormat, since method is sensitive to characters
    that occur before bullets.\r\n           if (pageIndex > 0)\r\n            {\r\n               
    KRUSE_TMP.text += KRUSE_TMP.text = KRUSE_TMP.text.Insert(0, RICH_TEXT_OVERFLOW[pageIndex
    - 1] == null ? \"\" : RICH_TEXT_OVERFLOW[pageIndex - 1]);\r\n            }\r\n\r\n          
    KRUSE_TMP.text = KRUSE_TMP.text.Insert(0, $\"<size={fontSize}>\");\r\n          
    // CANDLE_TMP.text = CANDLE_TMP.text.Insert(0, $\"<font=\\\"{fontType}\\\"><size={fontSize}>\");\r\n\r\n\r\n          
    ReplaceCandleText(KRUSE_TMP);\r\n\r\n            RichTextOverflow();\r\n          
    RichTextArrayCompression();\r\n\r\n            overflowAudit++;\r\n       }\r\n       
    else\r\n        {\r\n            overflowAudit = -1;\r\n\r\n           pageIndex
    = endCalibrationMemory == -1 ? pageLength - 1 : endCalibrationMemory;\r\n          
    pageIndex = Mathf.Clamp(pageIndex, 0, pageLength - 1);\r\n\r\n            Debug.Log($\"Loop
    Overflow Audit Complete | Page Index Assigned: {pageIndex}\");\r\n           
    endCalibrationMemory = 0;\r\n\r\n            if (emergencyUpdate)\r\n           
    {\r\n                DefinePage(pageIndex, true);\r\n                emergencyUpdate
    = false;\r\n            }\r\n            CANDLE_GAMEOBJECT.SetActive(true);\r\n          
    DOUBLE_GAMEOBJECT.SetActive(true);\r\n        }\r\n    }\r\n\r\n    public void
    FormatText(bool isForceLastCharNewLine)\r\n    {\r\n        for (int i = 0; i
    < FORMAT_FLAGS.Length; i++)\r\n        {\r\n            KRUSE_TMP.text = KRUSE_TMP.text.Replace(FORMAT_FLAGS[i],
    FORMAT_BRACKETS[i]);\r\n        }\r\n       \r\n        bool isCharacterAfterNewLine
    = false;\r\n        bool isBulleting = false;\r\n        bool isBreakBullet =
    false;\r\n        bool isDelayedBreakBullet = false;\r\n        bool isDelayedReturnBullet
    = false;\r\n        bool isTabbedBullet = false;\r\n        bool isIgnoreNextBullet
    = false;\r\n        int rightInsertStatusCode = 0;\r\n        char firstCharacterThisPage
    = KRUSE_TMP.text[0];\r\n        char firstCharacterNextPage = TEXT.text[SLICE_LIST[pageIndex
    + 1]];\r\n        string insertString = \"\";\r\n        \r\n        if (pageIndex
    > 0)\r\n        {\r\n           string richTextOverflow = RICH_TEXT_OVERFLOW[pageIndex
    - 1];\r\n            if (richTextOverflow.Contains(\"<indent=\"))\r\n           
    {\r\n                isDelayedBreakBullet = true;\r\n                isDelayedReturnBullet
    = true;\r\n                isBulleting = true;\r\n            }\r\n           
    else if (richTextOverflow.Contains(FORMAT_BRACKETS[Array.IndexOf(FORMAT_FLAGS,
    \"<START>\")]) && firstCharacterThisPage != '\\n')\r\n            {\r\n               
    isCharacterAfterNewLine = true;\r\n            }\r\n        }\r\n        if (isForceLastCharNewLine
    && firstCharacterNextPage == '\\n')\r\n        {\r\n            KRUSE_TMP.text
    += \"\\n\";\r\n        }\r\n        \r\n        if (pageIndex == 46 && blockIndex
    == 34)\r\n        {\r\n            Debug.Log($\"isDelayedBreakBullet: {isDelayedBreakBullet}
    | isDelayedReturnBullet: {isDelayedReturnBullet} | isBulleting: {isBulleting}
    | isCharacterAfterNewLine: {isCharacterAfterNewLine}  | That's All Folks!\");\r\n      
